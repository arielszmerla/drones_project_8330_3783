<Window x:Class="PL1.ParcelActionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL1"
        mc:Ignorable="d"
        Title="Parcel Action Window" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="\images\parcel.png" />
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"  />
            <RowDefinition Height="*"  />
        </Grid.RowDefinitions>
        <StackPanel x:Name="Add_Stack" Grid.Row="0">


        </StackPanel>
        <StackPanel x:Name="Update_Stack" Grid.Row="0">


        </StackPanel>
        <TextBox x:Name="show_parcel" HorizontalAlignment="Left" Margin="274,5,0,0" Grid.Row="1" IsEnabled="True" IsReadOnly="False" TextWrapping="Wrap" VerticalAlignment="Top" Width="246" Height="149" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
